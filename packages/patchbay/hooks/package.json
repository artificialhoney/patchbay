{
  "name": "@patchbay/hooks",
  "description": "Please enter a description for your extension",
  "icon": "extension",
  "version": "1.0.0",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-extension-hook"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "directus:extension": {
    "id": "@patchbay/hooks",
    "type": "hook",
    "path": "dist/index.js",
    "source": "src/index.js",
    "host": "^11.7.2",
    "hidden": false
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link",
    "validate": "directus-extension validate"
  },
  "dependencies": {
    "@patchbay/theme": "workspace:*"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "13.1.0"
  }
}
