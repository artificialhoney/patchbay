{
  "name": "@cables/ui",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@cables/api": "workspace:*",
    "@cables/cables": "workspace:*",
    "@cables/client": "workspace:*",
    "chroma-js": "2.1.0",
    "error-stack-parser": "2.1.4",
    "gl-matrix": "3.1.0",
    "intro.js": "2.9.0",
    "izitoast": "1.1.4",
    "loadjs": "4.3.0",
    "marked": "12.0.2",
    "moment-mini": "2.29.4",
    "pako": "2.0.3",
    "platform": "1.3.6",
    "raphael": "2.2.0",
    "socketcluster-client": "16.0.1",
    "sortablejs": "1.15.2",
    "tinysort": "3.2.8"
